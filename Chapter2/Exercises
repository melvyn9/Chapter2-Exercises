time = 1400
waittime = 5100
normalday = 2400

waittime1 = waittime - normalday
waittime2 = waittime1 - normalday
newtime = time + waittime2
print("The new time is", newtime)

Currenttime = int(input("Please enter the 4 digit current time: "))
time2 = int(input("Please enter the 4 digit number of hours to wait: "))
Waittime = time2

while Waittime > 24000 :
    Waittime2 = Waittime - 2400

Newtime = Currenttime + Waittime

