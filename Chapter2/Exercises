#All work and no play makes Jack a dull boy

a = "All"
w = "work"
z = "and"
n = "no"
p = "play"
m = "makes"
j = "Jack"
d = "a"
e = "dull"
b = "boy"

print(a,w,z,n,p,m,j,d,e,b)

print(6 * (1 - 2))

